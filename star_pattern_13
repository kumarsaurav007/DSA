"""
* * * * 
*     * 
*     * 
* * * * 
"""

n = 4
for i in range(1):
    for j in range(n):
        print("*",end=" ")
    print()
for i in range(2,4):
    for j in range(n):
        if j ==0 or j==(n-1):
            print("*",end=" ")
        else:
            print(" ",end=" ")
    print()
for i in range(3,n):
    for j in range(n):
        print("*",end=" ")
    print()

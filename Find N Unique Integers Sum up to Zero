def sumZero(self, n: int) -> List[int]:
        output = []
        if n % 2 == 0:
            for i in range(1, n // 2 + 1):
                output.append(i)
                output.append(-i)
        else:
            for i in range(0 , n // 2 + 1):
                if i == 0:
                    output.append(i)
                else:
                    output.append(i)
                    output.append(-i)
        return output

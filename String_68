# Sort Vowels in a String :-

def sortVowels(self, s: str) -> str:
        l = []
        for i in s:
            if i in "aeiouAEIOU":
                l.append(i)
        
        l.sort()
        new_s = ""
        for i in s:
            if i in "aeiouAEIOU":
                new_s += l[0]
                l.pop(0)
            else:
                new_s += i

        return new_s

# Smallest Integer Divisible by K :-

def smallestRepunitDivByK(self, k: int) -> int:
        if k % 2 == 0 or k % 5 == 0:
            return -1

        cur = 1 % k
        length = 1
        while cur != 0:
            cur = (cur * 10 + 1) % k
            length += 1
            if length > k:  
                return -1
        return length

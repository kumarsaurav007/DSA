"""
12345 54321
1234* *4321
123** **321
12*** ***21
1**** ****1
"""

n = 5
for i in range(n):
    p=1
    for j in range(n-i):
        print(p,end="")
        p+=1
    for j in range(i):
        print("*", end="")
    for j in range(1):
        print(" ",end="")
    for j in range(i):
        print("*",end="")
    p = 5
    for j in range(n-i):
        print(p-i,end="")
        p-=1
    print()

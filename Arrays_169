# Delete Columns to Make Sorted :-

def minDeletionSize(self, strs: List[str]) -> int:
        rows = len(strs)
        columns = len(strs[0])

        count = 0

        for j in range(columns):
            for i in range(1,rows):
                if strs[i][j]<strs[i-1][j]:
                    count+=1
                    break
        return count

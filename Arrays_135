# Bitwise ORs of Subarrays :-

def subarrayBitwiseORs(self, arr: List[int]) -> int:
        res = set()
        cur = set()
        
        for num in arr:
            # Update cur with OR of current num with all previous ORs
            cur = {num | prev for prev in cur} | {num}
            res.update(cur)
        
        return len(res)

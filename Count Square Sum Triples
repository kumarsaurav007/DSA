def countTriples(self, n: int) -> int:
        squares = {i*i: i for i in range(1, n+1)}

        ans = 0
        for a in range(1, n+1):
            for b in range(1, n+1):
                s = a*a + b*b
                if s in squares and squares[s] <= n:
                    ans += 1
            
        return ans

# Four Divisors :-

def sumFourDivisors(self, nums: List[int]) -> int:
        ans = 0
        for i in nums:
            divs = set()
            for j in range(1, int(math.sqrt(i)) + 1):
                if i % j == 0:
                    divs.add(j)
                    divs.add(i // j)
                    if len(divs) > 4:
                        break
            
            if len(divs) == 4:
                ans += sum(divs)

        return ans

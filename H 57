# Smallest Missing Non-negative Integer After Operations :-

def findSmallestInteger(self, nums: List[int], value: int) -> int:
        freq = Counter()

        for num in nums:
            mod = num % value
            freq[mod] += 1

        i = 0
        while True:
            mod = i % value
            if freq[mod] == 0:
                return i
            freq[mod] -= 1
            i += 1

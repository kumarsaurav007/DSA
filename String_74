# Number of Substrings With Only 1s :-

def numSub(self, s: str) -> int:
        MOD = 10 ** 9 + 7
        n = 0
        ans = 0
        for i in s:
            if i == "0":
                ans = (ans + n * (n + 1) // 2) % MOD
                n = 0
            else:
                n += 1
        ans = (ans + n * (n + 1) // 2) % MOD
        return ans

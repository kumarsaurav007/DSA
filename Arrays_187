# Transformed Array :-

def constructTransformedArray(self, nums: List[int]) -> List[int]:
        n = len(nums)
        result = [0] * n
        
        for i in range(n):
            if nums[i] == 0:
                result[i] = 0
            else:
                target_index = ((i + nums[i]) % n + n) % n
                result[i] = nums[target_index]
                
        return result

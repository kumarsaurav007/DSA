# Greatest Sum Divisible by Three :-

def maxSumDivThree(self, nums: List[int]) -> int:
        dp = [0, float('-inf'), float('-inf')]

        for x in nums:
            new_dp = dp[:] 

            for r in range(3):
                new_r = (r + x % 3) % 3
                new_dp[new_r] = max(new_dp[new_r], dp[r] + x)

            dp = new_dp

        return dp[0]

# Prime Number of Set Bits in Binary Representation :-

def countPrimeSetBits(self, left: int, right: int) -> int:
        prime = [2, 3, 5, 7, 11, 13, 17, 19]
        ans = 0

        for i in range(left, right + 1):
            bit_i = str(bin(i))
            count_bit = bit_i.count("1")
            if int(count_bit) in prime:
                ans += 1

        return ans

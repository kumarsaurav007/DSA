# Trionic Array I :-

def isTrionic(self, nums: List[int]) -> bool:
        n = len(nums)
        i = 1

        # strictly increasing
        while i < n and nums[i] > nums[i - 1]:
            i += 1
        if i == 1 or i == n:
            return False

        # strictly decreasing
        while i < n and nums[i] < nums[i - 1]:
            i += 1
        if i == n:
            return False

        # strictly increasing
        while i < n and nums[i] > nums[i - 1]:
            i += 1

        return i == n

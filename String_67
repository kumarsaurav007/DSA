# Maximum Number of Words You Can Type :-

def canBeTypedWords(self, text: str, brokenLetters: str) -> int:
        count = 0
        list_text = text.split(" ")
        for i in range(len(list_text)):
            for j in brokenLetters:
                if j in list_text[i]:
                    count += 1
                    break
        
        return len(list_text) - count
